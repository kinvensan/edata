CREATE TABLE t_game_total_moneys (
  id bigint  ,
  uid bigint ,
  creditId string ,
  gameCode string,
  serverCode string ,
  efunOrderId string,
  thirdOrderId string,
  payType string ,
  currency string ,
  gamePay float ,
  stone float ,
  twCoin float ,
  usd float ,
  orderTime string ,
  flag string,
  payFrom string,
  remark string,
  area string,
  modifiedTime string ,
  resend string ,
  checkoutMonth string,
  roleLevel string,
  gamePoint float ,
  donate float ,
  reserve1 string,
  reserve2 string,
  reserve3 string,
  reserve4 string
) ROW FORMAT DELIMITED FIELDS TERMINATED BY ',' STORED AS TEXTFILE;


LOAD DATA LOCAL INPATH '/data/hkdata/t_game_total_moneys.sql' INTO TABLE t_game_total_moneys;

