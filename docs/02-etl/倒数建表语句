CREATE TABLE t_users_total_2013 (
  uid bigint,
  userid bigint,
  gpid int ,
  appPlatform string ,
  userName string ,
  userMac string ,
  userPwd string ,
  email string  ,
  address string  ,
  idCard string ,
  telephone string ,
  msnqq string ,
  registerTime string ,
  registerIp string ,
  lastLoginIp string ,
  lastLoginTime string ,
  loginIp string ,
  loginTime string ,
  modifiedTime string ,
  flag string ,
  gameCode string ,
  thirdPlate string ,
  platForm string ,
  advertiser string  ,
  area string  
) ROW FORMAT DELIMITED FIELDS TERMINATED BY ',' STORED AS TEXTFILE;


LOAD DATA LOCAL INPATH '/data/hkdata/t_users_total_2013.sql' INTO TABLE t_users_total_2013;


