CREATE TABLE t_users_total_relation (
  id bigint  ,
  userid bigint ,
  gpid int,
  appPlatform string ,
  gameCode string  ,
  thirdPlate string,
  plateForm string ,
  registerIp string,
  address string ,
  advertiser string ,
  mac string ,
  imei string ,
  androidid string ,
  systemVersion string ,
  deviceType string ,
  registerTime string ,
  flag int,
  area string 
)  ROW FORMAT DELIMITED FIELDS TERMINATED BY ',' STORED AS TEXTFILE;


LOAD DATA LOCAL INPATH '/data/hkdata/t_users_total_relation.sql' INTO TABLE t_users_total_relation;